<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LambdaView">
        <attr name="LambdaViewBorderWidth" format="dimension"/>
        <attr name="LambdaViewRectPadding" format="dimension"/>
        <attr name="LambdaViewRectSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="TPSView">
        <attr name="TPSViewBorderWidth" format="dimension" />
        <attr name="TPSViewRectPadding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RPMView">
        <attr name="RPMViewBorderWidth" format="dimension" />
        <attr name="RPMViewRectPadding" format="dimension" />
        <attr name="RPMViewMarkerTextPadding" format="dimension" />
        <attr name="RPMViewMarkerTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ActuatorView">
        <attr name="ActuatorViewBarBottomPadding" format="dimension" />
        <attr name="ActuatorViewBorderWidth" format="dimension" />
        <attr name="ActuatorViewRectPadding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ExpandablePanel">
        <attr name="handle" format="reference" />
        <attr name="content" format="reference" />
        <attr name="contentVisible" format="reference" />
        <attr name="collapsedHeight" format="dimension"/>
        <attr name="animationDuration" format="integer"/>
        <attr name="expandableText" format="string"/>
        <attr name="unitText" format="string"/>
        <attr name="auto" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="GraphRow">
        <attr name="XTitle" format="string"/>
        <attr name="YTitle" format="string"/>
    </declare-styleable>
</resources>